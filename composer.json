{
    "name": "nunomaduro/laravel-console-menu",
    "description": "Laravel Console Menu is an output method for your Laravel/Laravel Zero commands.",
    "keywords": [
        "console",
        "command-line",
        "php",
        "cli",
        "laravel-zero",
        "laravel",
        "artisan",
        "symfony"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/laravel-console-menu/issues",
        "source": "https://github.com/nunomaduro/laravel-console-menu"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "php-school/cli-menu": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "NunoMaduro\\LaravelConsoleMenu\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NunoMaduro\\LaravelConsoleMenu\\LaravelConsoleMenuServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.4"
    }
}
